{
  "name": "vff-extension-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prebuild": "rimraf dist",
    "watch": "webpack -w",
    "build": "npm-run-all --parallel build:*",
    "!build:main": "babel --copy-files --out-dir npmdist --ignore *.test.js src",
    "build:umd": "webpack",
    "!build:umd.min": "webpack --output-filename vff.min.js -p"
  },
  "files": [
    "dist",
    "test/setup.js"
  ],
  "keywords": [
    "vff",
    "broadcast",
    "graphics"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TwiztedDesign/vff-extension-boilerplate.git"
  },
  "author": "Danny Umansky <dok.umansky@twizted-design.com> (http://www.twizted-design.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TwiztedDesign/vff-extension-boilerplate/issues"
  },
  "homepage": "https://github.com/TwiztedDesign/vff-extension-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "css-loader": "0.28.9",
    "eslint": "4.15.0",
    "eslint-loader": "1.9.0",
    "fs-extra": "5.0.0",
    "ghooks": "2.0.2",
    "jasmine": "3.4.0",
    "jasmine-core": "3.4.0",
    "json-loader": "0.5.7",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.0.0",
    "karma-jasmine": "2.0.1",
    "karma-webpack": "^4.0.2",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "regenerator-runtime": "0.11.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "script-loader": "0.7.2",
    "style-loader": "0.20.1",
    "webpack": "3.10.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "3.7.2",
    "webpack-shell-plugin": "0.5.0"
  },
